doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Object Table
        a(href="/update-cobj") Add/Update this table
        table
            thead
                tr
                    th Name
                    th Type
                    th Preferred food
                    th
            tbody
                each contact in data
                    tr
                        td #{contact.properties.pet_name}
                        td #{contact.properties.pet_type}
                        td #{contact.properties.preferred_food}
                        //- td
                            a(href=`/update/${contact.id}`) Update